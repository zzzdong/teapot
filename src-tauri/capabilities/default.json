{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "permissions for HTTP requests and store using Tauri plugins",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "http:allow-fetch",
    "http:allow-fetch-cancel",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send",
    "http:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-delete",
    "store:allow-clear",
    "store:allow-has",
    "store:allow-keys",
    "store:allow-values",
    "store:allow-length",
    "store:allow-get-store"
  ]
}